{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "enginges": {
    "node": "8.1.1",
    "yarn": "1.13.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "YARN_CONFIG_PRODUCTION=false yarn --cwd ./client && yarn build --cwd ./client"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.19",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-google-oauth20": "2",
    "stripe": "^6.28.0"
  }
}
